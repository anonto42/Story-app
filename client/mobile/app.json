{
  "expo": {
    "name": "Story App",
    "slug": "story-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/main.png",
    "scheme": "story-app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/main.png",
        "backgroundColor": "#071B49"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/main.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/main.png",
          "imageWidth": 334,
          "resizeMode": "contain",
          "backgroundColor": "#071B49"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
